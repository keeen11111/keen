  <equality>
    <joint name="eq_steer_fl" joint1="steer_wheel" joint2="fl_steer" polycoef="0 0.06 0 0 0"/>
    <joint name="eq_steer_fr" joint1="steer_wheel" joint2="fr_steer" polycoef="0 0.06 0 0 0"/>
