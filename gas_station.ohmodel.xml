    <body name="robot" pos="-6 -4 0">
      <joint name="robot_x" type="slide" axis="1 0 0" range="-40 40"/>
      <joint name="robot_y" type="slide" axis="0 1 0" range="-40 40"/>
      <joint name="robot_yaw" type="hinge" axis="0 0 1" range="-180 180"/>

      <geom class="robot" name="robot_base" type="box" pos="0 0 0.25" size="0.32 0.26 0.25" material="mat_metal"/>
