      </body>

      <body name="window_right" pos="-0.35 -0.86 0.92">
        <joint name="window_r" type="slide" axis="0 0 1" range="0 0.28" damping="3"/>
        <geom class="car" type="box" pos="0 0 0.12" size="0.75 0.02 0.2" material="mat_glass"/>
      </body>

      <body name="fl_steer" pos="1.35 0.78 0.22">
