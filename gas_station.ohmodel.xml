      <geom class="robot" name="robot_wheel_r" type="cylinder" pos="0 -0.28 0.12" size="0.10 0.05" rgba="0.08 0.08 0.08 1"/>

      <body name="torso" pos="0 0 0.62">
        <geom class="robot" type="box" size="0.22 0.18 0.28" material="mat_metal"/>

        <body name="head" pos="0 0 0.42">
          <joint name="head_yaw" type="hinge" axis="0 0 1" range="-90 90" damping="1"/>
